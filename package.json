{
  "name": "eth0s",
  "version": "0.0.3",
  "description": "An Ethereum Zero Client",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "package-osx": "electron-packager ./ Eth0s --platform=darwin --arch=x64 --out ./dist --overwrite --icon=src/client/images/icon.icns",
    "package-win": "electron-packager ./ Eth0s --platform=win32 --arch=x64 --out ./dist --overwrite --icon=src/client/images/icon.icns",
    "package": "electron-packager ./ Eth0s --all --out ./dist --overwrite --icon=src/client/images/icon.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d11e9/eth0s/issues"
  },
  "homepage": "https://github.com/d11e9/eth0s#readme",
  "devDependencies": {
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.0"
  },
  "dependencies": {
    "ipfs-js": "0.0.9",
    "jquery": "^2.2.3",
    "querystring": "^0.2.0",
    "request": "^2.72.0"
  }
}
